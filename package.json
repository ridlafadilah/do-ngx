{
  "name": "dongkap",
  "version": "0.0.0",
  "scripts": {
    "start:do-ngx": "ng serve @dongkap/do-ngx --port 4242 --base-href=/",
    "start:do-ngx:heroku": "ng serve @dongkap/do-ngx --port 4242 --base-href=/ --configuration=heroku",
    "clean:do-core": "node_modules/.bin/rimraf dist/@dongkap/do-core",
    "clean:do-theme": "node_modules/.bin/rimraf dist/@dongkap/do-theme",
    "clean:do-common": "node_modules/.bin/rimraf dist/@dongkap/do-common",
    "clean:do-storage": "node_modules/.bin/rimraf dist/@dongkap/do-storage",
    "clean:do-auth": "node_modules/.bin/rimraf dist/@dongkap/do-auth",
    "clean:do-extra": "node_modules/.bin/rimraf dist/@dongkap/do-extra",
    "clean:do-sys": "node_modules/.bin/rimraf dist/@dongkap/do-sys",
    "clean:do-exercise": "node_modules/.bin/rimraf dist/@dongkap/do-exercise",
    "clean:do-ngx": "node_modules/.bin/rimraf dist/@dongkap/do-ngx",
    "build:do-core": "ng build @dongkap/do-core --prod",
    "build:do-theme": "ng build @dongkap/do-theme --prod",
    "build:do-common": "ng build @dongkap/do-common --prod",
    "build:do-storage": "ng build @dongkap/do-storage --prod",
    "build:do-auth": "ng build @dongkap/do-auth --prod",
    "build:do-extra": "ng build @dongkap/do-extra --prod",
    "build:do-sys": "ng build @dongkap/do-sys --prod",
    "build:do-exercise": "ng build @dongkap/do-exercise --prod",
    "copy:do-theme": "node_modules/.bin/cpx \"./@dongkap/do-theme/src/styles/**/*\" \"./dist/@dongkap/do-theme/styles\"",
    "copy:do-common-assets": "node_modules/.bin/cpx \"./@dongkap/do-common/src/assets/**/*\" \"./dist/@dongkap/do-common/assets/\"",
    "copy:do-common-styles": "node_modules/.bin/cpx \"./@dongkap/do-common/src/styles/**/*\" \"./dist/@dongkap/do-common/styles/\"",
    "p:do-core": "npm run clean:do-core && npm run build:do-core && cd dist/@dongkap/do-core && npm pack",
    "p:do-theme": "npm run clean:do-theme && npm run build:do-theme && npm run copy:do-theme && cd dist/@dongkap/do-theme && npm pack",
    "p:do-common": "npm run clean:do-common && npm run build:do-common && npm run copy:do-common-assets && npm run copy:do-common-styles && cd dist/@dongkap/do-common && npm pack",
    "p:do-storage": "npm run clean:do-storage && npm run build:do-storage && cd dist/@dongkap/do-storage && npm pack",
    "p:do-auth": "npm run clean:do-auth && npm run build:do-auth && cd dist/@dongkap/do-auth && npm pack",
    "p:do-extra": "npm run clean:do-extra && npm run build:do-extra && cd dist/@dongkap/do-extra && npm pack",
    "p:do-sys": "npm run clean:do-sys && npm run build:do-sys && cd dist/@dongkap/do-sys && npm pack",
    "p:do-exercise": "npm run clean:do-exercise && npm run build:do-exercise && cd dist/@dongkap/do-exercise && npm pack",
    "install:do-core": "npm run p:do-core && npm r @dongkap/do-core && npm i -O ./dist/@dongkap/do-core/dongkap-do-core-0.0.1.tgz",
    "install:do-theme": "npm run p:do-theme && npm r @dongkap/do-theme && npm i -O ./dist/@dongkap/do-theme/dongkap-do-theme-0.0.1.tgz",
    "install:do-common": "npm run p:do-common && npm r @dongkap/do-common && npm i -O ./dist/@dongkap/do-common/dongkap-do-common-0.0.1.tgz",
    "install:do-storage": "npm run p:do-storage && npm r @dongkap/do-storage && npm i -O ./dist/@dongkap/do-storage/dongkap-do-storage-0.0.1.tgz",
    "install:do-auth": "npm run p:do-auth && npm r @dongkap/do-auth && npm i -O ./dist/@dongkap/do-auth/dongkap-do-auth-0.0.1.tgz",
    "install:do-extra": "npm run p:do-extra && npm r @dongkap/do-extra && npm i -O ./dist/@dongkap/do-extra/dongkap-do-extra-0.0.1.tgz",
    "install:do-sys": "npm run p:do-sys && npm r @dongkap/do-sys && npm i -O ./dist/@dongkap/do-sys/dongkap-do-sys-0.0.1.tgz",
    "install:do-exercise": "npm run p:do-exercise && npm r @dongkap/do-exercise && npm i -O ./dist/@dongkap/do-exercise/dongkap-do-exercise-0.0.1.tgz",
    "install:all-lib": "npm run install:do-core && npm run install:do-theme && npm run install:do-common && npm run install:do-storage && npm run install:do-auth && npm run install:do-extra && npm run install:do-sys && npm run install:do-exercise",
    "build:do-ngx": "npm run clean:do-ngx && ng build @dongkap/do-ngx --prod",
    "build:do-ngx:github": "ng build @dongkap/do-ngx --configuration=github --base-href=/do-ngx/ --outputPath=dist --deleteOutputPath=false",
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./@dongkap/do-ngx/src/tsconfig.app.json\""
  },
  "private": true,
